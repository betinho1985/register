<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Resultados Digitais</title>

  <!-- Bootstrap Core CSS -->
  <link href="../assets/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom CSS -->
  <link href="../assets/css/sb-admin.css" rel="stylesheet">

  <!-- Custom Fonts -->
  <link href="../assets/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
  <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->

  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

</head>

<body>

<div id="wrapper">

  <!-- Navigation -->
  <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">

      <a class="navbar-brand" href="#">Register</a>

    </div>
    <!-- Top Menu Items -->
    <ul class="nav navbar-right top-nav">

      <li class="dropdown">
        <% if current_user %>
            <%= link_to "Logout #{current_user.email}", :logout, method: :post %>
        <%end%>


      </li>
    </ul>

    <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
    <div class="collapse navbar-collapse navbar-ex1-collapse">
      <ul class="nav navbar-nav side-nav">
        <% if current_user %>
            <li>
              <a href="../contacts"><i class="fa fa-fw fa-table"></i> Contacts</a>
            </li>
            <li>
              <a href="#"><i class="fa fa-fw fa-edit"></i> New Contacts</a>
            </li>


        <%else%>
            <li>
              <a href="#"><i class="fa fa-fw fa-table"></i> Contacts</a>
            </li>
            <li>
              <a href="#"><i class="fa fa-fw fa-edit"></i> New Contacts</a>
            </li>

            <li class="active">
              <a href="#"><i class="fa fa-fw fa-file"></i> Login</a>
            </li>

        <%end%>
      </ul>
    </div>
    <!-- /.navbar-collapse -->
  </nav>
<div id="page-wrapper">

<div class="container-fluid">

<!-- Page Heading -->
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">
      Novo Contato
    </h1>

  </div>
</div>
<!-- /.row -->

<div class="row">

  <%= form_for(@contact) do |f| %>
  <div class="col-lg-6">
      <div class="form-group" >
        <%= f.label :name %><br />
        <%= f.text_field :name %>
      </div>
      <div class="form-group">
        <%= f.label :company %><br />
        <%= f.text_field :company %>
      </div>
      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.text_field :email%>
      </div>
      <div class="form-group">
        <%= f.label :website %><br />
        <%= f.text_field :website%>
      </div>


      <div class="actions">
        <%= f.submit %>
      </div>
    </div>
      <div class="col-lg-6">
        <div class="form-group">
          <%= f.label :last_name %><br />
          <%= f.text_field :last_name %>
        </div>
        <div class="form-group">
          <%= f.label :job_title %><br />
          <%= f.text_field :job_title%>
        </div>
        <div class="form-group">
          <%= f.label :phone %><br />
          <%= f.text_field :phone%>
        </div>

      </div>
  <% end %>



</div>
<!-- /.row -->

</div>
<!-- /.container-fluid -->

</div>
<!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery Version 1.11.0 -->
<script src="../js/jquery-1.11.0.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="../js/bootstrap.min.js"></script>

</body>

</html>